[[model]]
# Initial values
ikr.O = 0

[ikr]
# Voltage-dependent transition rates
alpha = p1 * exp(p2 * membrane.V)
beta = p3 * exp(-p4 * membrane.V)

# Transition rate parameters
p1 = 2.1e-3
p2 = 7.2e-2
p3 = 3.5e-5
p4 = 6.2e-2

# Model ODE
dot(O) = alpha * C - beta * O
C = 1 - O

[engine]
time = 0 bind time
    in [ms]
pace = 0 bind pace

[membrane]
V = engine.pace
    desc: membrane potential
    in [mV]